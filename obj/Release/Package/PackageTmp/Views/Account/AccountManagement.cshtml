<style>
    .Zebra_DatePicker_Icon_Wrapper {
        width: 100% !important;
    }

    #StartDate, #EndDate {
        background: #fff;
    }
</style>

<!-- Page Content-->
<main class="page-content">
    @Html.Hidden("Login", @Url.Action("login", "Account"))
    @Html.Hidden("lobby", @Url.Action("lobby", "home"))
    @Html.Hidden("APIURL", @System.Configuration.ConfigurationManager.AppSettings["APIUrl"])

    <section>
        <div class="container">
            <ol class="breadcrumb">
                <li><a href="~/">home</a></li>
                <li class="active">account management</li>
            </ol>
        </div>
    </section>
    <section class="section-bottom-30">
        <div class="container">
            <h4>account management</h4>

            <div class="row">
                <ul class="nav nav-tabs" role="tablist">
                    <li class="active"><a data-toggle="tab" href="#account">Account</a></li>
                    <li class="dropdown" style="cursor:pointer;">
                        <a class="dropdown-toggle" data-toggle="dropdown">
                            Account Info
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a data-toggle="tab" href="#account-details">Account Details</a></li>
                            <li><a data-toggle="tab" href="#transactions" id="transactions-menu">Transactions</a></li>
                            <li><a data-toggle="tab" href="#limits" id="player-limits-menu" style="display:none;">Limits</a></li>

                        </ul>
                    </li>
                    <li class="dropdown" style="cursor:pointer;">
                        <a class="dropdown-toggle" data-toggle="dropdown">
                            Change Info
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            @*<li><a data-toggle="tab" href="#change-password">Change Password</a></li>*@
                            <li><a data-toggle="tab" href="#change-pin">Change PIN</a></li>
                        </ul>
                    </li>
                    <li><a data-toggle="tab" href="#redeem-voucher" id="redeem-voucher-menu" style="display:none;">Redeem Voucher</a></li>
                    <li><a data-toggle="tab" href="#ticket-replays" id="ticket-replay-menu" style="display:none;">Ticket Replays</a></li>
                    <li><a data-toggle="tab" href="#syndicate-orders" id="syndicate-orders-menu" style="display:none;">Syndicate Orders</a></li>
                    <li><a data-toggle="tab" href="#withdraw-funds" style="display:none;">Withdraw Funds</a></li>
                    <li><a data-toggle="tab" href="#support" id="support-menu" style="display:none;">Support</a></li>
                </ul>
                <div id="message" style="display:none;"></div>
                <div class="tab-content">
                    <div id="account" class="tab-pane fade in active">
                        <h3>Account</h3>
                        <hr />
                        <div class="form-inline">
                            <div class="row">
                                <div class="col-sm-2">
                                    Customer ID:
                                </div>
                                <div class="col-sm-10">
                                    <span id="account-customer-id"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-2">
                                    Username:
                                </div>
                                <div class="col-sm-10">
                                    <span id="account-customer-username"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-2">
                                    Full Name:
                                </div>
                                <div class="col-sm-10">
                                    <span id="account-customer-full-name"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-2">
                                    Cash Balance:
                                </div>
                                <div class="col-sm-10">
                                    <span id="account-customer-balance"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-2">
                                    Currency:
                                </div>
                                <div class="col-sm-10">
                                    <span id="account-customer-currency"></span>
                                </div>
                            </div>

                            <br />
                            <div class="row" id="div-email-validation-request" hidden>
                                <div class="col-sm-12">
                                    <p>
                                        <b class="alert-danger">Your email address requires validation for gameplay!</b><br />
                                        <a href="#" onclick="sendValidationMessage(11)">Click here</a> to have the system send a validation message to your registered email address.
                                    </p>
                                </div>
                            </div>
                            <div class="row" id="div-email-validation-confirm" hidden>
                                <div class="col-sm-12">
                                    <input type="text" class="form-control" id="ValidateEmailCode" placeholder="Enter Email Code Here" />
                                    <a href="#" class="btn btn-primary btn-details-validate-email">Validate Code</a><br />
                                    <a href="#" id="resend-code-email" onclick="sendValidationMessage(11)" hidden>Resend Email Code</a>
                                </div>
                            </div>
                            <br />
                            <div class="row" id="div-cellphone-validation-request" hidden>
                                <div class="col-sm-12">
                                    <p>
                                        <b class="alert-danger">Your cell phone requires validation for gameplay!</b><br />
                                        <a href="#" onclick="sendValidationMessage(10)">Click here</a> to have the system send a validation SMS message to your registered cell phone. Your number must be in international format <a href="https://en.wikipedia.org/wiki/MSISDN" target="_blank">(MSISDN)</a> to send.
                                    </p>
                                </div>
                            </div>
                            <div class="row" id="div-cellphone-validation-confirm" hidden>
                                <div class="col-sm-12">
                                    <input type="text" class="form-control" id="ValidateCellPhoneCode" placeholder="Enter SMS Code Here" />
                                    <a href="#" class="btn btn-primary btn-details-validate-cellphone">Validate Code</a><br />
                                    <a href="#" id="resend-code-cellphone" onclick="sendValidationMessage(10)" hidden>Resend SMS Code</a>
                                </div>
                            </div>

                        </div>
                    </div>
                    <div id="account-details" class="tab-pane fade">
                        <h3>Account Details</h3>
                        <hr />
                        <input data-val="true" data-val-number="The field ID must be a number." data-val-required="The ID field is required." id="ID" name="ID" type="hidden" value="2" />
                        <div class="row">
                            <div class="col-sm-3">
                                <span><b>First Name:</b></span><br />
                                <input type="text" class="form-control" id="FirstName" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="FirstName" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Last Name:</b></span><br />
                                <input type="text" class="form-control" id="LastName" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="LastName" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Gender:</b></span><br />

                                <input type="text" class="form-control" id="Gender" disabled />
                                <span class="field-validation-valid text-danger" data-valmsg-for="Gender" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Birthday:</b></span><br />
                                <div class="input-group date dateonly field-datetime" data-date-format="m/d/yyyy">
                                    <input type="text" class="form-control field-datetime" id="Birthday" disabled />
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                </div>
                                <span class="field-validation-valid text-danger" data-valmsg-for="Birthday" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-3">
                                <span><b>Email:</b></span><br />
                                <input type="text" class="form-control" id="EmailAddress" />
                            </div>
                            <div class="col-sm-3">
                                <span><b>Cell Phone:</b></span><br />
                                <input type="text" class="form-control" id="CellPhone" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="CellPhone" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Alternate Phone:</b></span><br />
                                <input type="text" class="form-control" id="PhoneNumber" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="PhoneNumber" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Employer:</b></span><br />
                                <input type="text" class="form-control" id="Employer" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="Employer" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-3">
                                <span><b>Address Line 1:</b></span><br />
                                <input type="text" class="form-control" id="AddressLine1" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="AddressLine1" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Address Line 2:</b></span><br />
                                <input type="text" class="form-control" id="AddressLine2" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="AddressLine2" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Address Line 3:</b></span><br />
                                <input type="text" class="form-control" id="AddressLine3" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="AddressLine3" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Address Line 4:</b></span><br />
                                <input type="text" class="form-control" id="AddressLine4" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="AddressLine4" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-3">
                                <span><b>Locality/City:</b></span><br />
                                <input type="text" class="form-control" id="Locality" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="Locality" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>State:</b></span><br />
                                <input type="text" class="form-control" id="Region" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="Region" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Postcode:</b></span><br />
                                <input type="text" class="form-control" id="Postcode" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="Postcode" data-valmsg-replace="true"></span>
                            </div>
                            <div class="col-sm-3">
                                <span><b>Country:</b></span><br />
                                <input type="text" class="form-control" id="Country" />
                                <span class="field-validation-valid text-danger" data-valmsg-for="Country" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                        <br />

                        <br />
                        <div>
                            <a href="#" class="btn btn-primary btn-enable-details-edit">Edit</a>
                            <a href="#" class="btn btn-primary btn-details-update" style="display:none;">Submit</a>
                        </div>

                    </div>
                    <div id="transactions" class="tab-pane fade">
                        <h3>Transactions</h3>
                        <hr />
                        <div class="row">

                            <div class="col-sm-3">
                                <span><b>Start Date:</b></span><br />
                                <div class="input-group" id="account-transaction-history-start-date">
                                    <input type="text" id="StartDate" class="form-control" placeholder="Start Date" value="" />
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                </div>
                            </div>
                            <div class="col-sm-3">
                                <span><b>End Date:</b></span><br />
                                <div class="input-group date dateonly field-datetime" id="account-transaction-history-end-date" data-date="" data-date-format="m/d/yyyy">
                                    <input type="text" id="EndDate" class="form-control" placeholder="End Date" value="" />
                                    <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                                </div>

                            </div>
                            <div class="col-sm-3">
                                <span>&nbsp;</span><br />
                                <span><button class="btn btn-primary btn-transaction-history-search" type="submit"><span class="glyphicon glyphicon-search"></span> Search</button></span>
                            </div>
                        </div>
                        <br />


                        <div class="table-responsive">
                            <table id="example" class="table table-striped table-hover table-bordered datatable">
                                <thead>
                                    <tr>
                                        <th class="no-wrap">Transaction Date</th>
                                        <th class="no-wrap">Order ID</th>

                                        <th class="no-wrap">Transaction</th>
                                        <th class="no-wrap text-right">Amount&nbsp;&nbsp;</th>
                                        <th class="no-wrap text-right">Pre Amount&nbsp;&nbsp;</th>
                                        <th class="no-wrap text-right">Post Amount&nbsp;&nbsp;</th>

                                    </tr>
                                </thead>
                                <tbody class="transaction-history-body"></tbody>
                            </table>
                            <template id="transaction-history-row-template">
                                <tr>
                                    <td class="no-wrap"><span class="transaction-date"></span></td>
                                    <td class="no-wrap"><span class="transaction-order-id"></span></td>

                                    <td class="no-wrap"><span class="transaction-summary"></span></td>
                                    <td class="no-wrap text-right"><span class="transaction-amount"></span></td>
                                    <td class="no-wrap text-right"><span class="transaction-pre-amount"></span></td>
                                    <td class="no-wrap text-right"><span class="transaction-post-amount"></span></td>

                                </tr>
                            </template>
                        </div>

                        <style>
                            /* Used to fix the datatable sizing, without it puts a bottom scroll-bar */
                            .form-inline {
                                width: 1095px;
                            }
                        </style>
                    </div>
                    <div id="limits" class="tab-pane fade">
                        <h3>Limits</h3>
                        <hr />
                        <p>
                            Limits control the maximum amount spent for wagers, losses, deposits and withdrawals.
                            If there is a system imposed limit on a category, your personal limit cannot be set on that category to be higher than the system's currently imposed limit.
                        </p>
                        <p>
                            For example, if the system has a limit on withdrawals to be $1,000/day, you cannot set your personal limit to more than $1,000.
                            Once your personal limits are set, they can be changed on request, but there is a 24 hours waiting period after the request is made.
                        </p>

