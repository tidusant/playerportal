                                        <input class="form-control" data-val="true" data-val-number="The field Amount must be a number." id="wd_d_Amount" name="wd_d.Amount" type="text" /><input id="wd_d_Frequency" name="wd_d.Frequency" type="hidden" value="D" /><input data-val="true" data-val-number="The field LimitTypeID must be a number." data-val-required="The LimitTypeID field is required." id="wd_d_LimitTypeID" name="wd_d.LimitTypeID" type="hidden" value="2" /><input id="wd_d_Name" name="wd_d.Name" type="hidden" value="Withdraw" />
